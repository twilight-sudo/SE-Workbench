    "@opentiny/vue-icon": "3.18.0",
    "@opentiny/vue-locale": "3.18.0",
    "@opentiny/vue-theme": "3.18.2",
    "@types/mockjs": "1.0.10",
    "@types/node": "22.7.4",
    "@vueuse/core": "10.11.1",
    "@vueuse/head": "2.0.0",
    "axios": "1.7.7",
    "dayjs": "1.11.13",
    "echarts": "5.4.3",
