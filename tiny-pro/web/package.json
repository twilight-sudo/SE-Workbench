    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-vue": "8.7.1",
    "husky": "7.0.4",
    "less": "4.2.0",
    "less-loader": "12.2.0",
    "lint-staged": "11.2.6",
    "mockjs": "1.1.0",
    "prettier": "3.3.3",
    "rollup-plugin-visualizer": "5.12.0",
