    "vite-plugin-style-import": "2.0.0",
    "vite-svg-loader": "4.0.0",
    "vue-tsc": "1.8.27"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "gifsicle": "5.2.0"
  }
